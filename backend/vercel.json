{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "functions": {
    "src/server.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "regions": ["gru1"],
  "env": {
    "NODE_ENV": "production"
  }
}